    "@angular/forms": "~12.0.1",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/platform-server": "^12.0.1",
    "@angular/router": "~12.0.1",
    "@ngrx/effects": "^12.0.0",
    "@ngrx/entity": "^12.0.0",
    "@ngrx/router-store": "^12.0.0",
    "@ngrx/store": "^12.0.0",
    "@ngrx/store-devtools": "^12.0.0",
    "@nguniversal/common": "^12.0.0",
    "@nguniversal/express-engine": "^12.0.0",
    "@tonaljs/tonal": "^4.6.0",
    "angular-audio-context": "^31.0.2",
    "angular2-draggable": "^2.3.2",
    "babylon-navigation-mesh": "^1.2.6",
    "babylonjs": "^4.0.3",
    "babylonjs-materials": "^4.0.3",
    "canvas": "^2.6.1",
    "chance": "^1.1.3",
    "express": "^4.17.1",
    "fast-simplex-noise": "^4.0.0",
    "jsdom": "^16.6.0",
    "ngx-device-detector": "^2.0.10",
