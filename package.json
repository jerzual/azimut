{
	"name": "@jerzual/azimut",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"serve": "ng serve",
		"test": "jest",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"test:watch": "jest --watch",
		"gen:config": "json2ts --input src/assets/config-schema.json --output src/assets/config.d.ts",
		"gen:sprites": "json2ts --input src/assets/sprites-schema.json --output src/assets/sprites.d.ts",
		"gen": "npm run gen:config && npm run gen:sprites",
		"serve:ssr": "ng run azimut:serve-ssr",
		"build": "ng build --configuration production && ng run azimut:server:production",
		"build:dev": "ng build && ng run azimut:server",
		"start": "node dist/server/main.js",
		"prerender": "ng run azimut:prerender"
	},
	"engines": {
		"node": "18.x"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^16.2.0",
		"@angular/cdk": "^16.2.0",
		"@angular/common": "^16.2.0",
		"@angular/compiler": "^16.2.0",
		"@angular/core": "^16.2.0",
		"@angular/forms": "^16.2.0",
		"@angular/platform-browser": "^16.2.0",
		"@angular/platform-browser-dynamic": "^16.2.0",
		"@angular/platform-server": "^16.2.0",
		"@angular/router": "^16.2.0",
		"@ngrx/component-store": "^16.2.0",
		"@ngrx/effects": "^16.2.0",
		"@ngrx/entity": "^16.2.0",
		"@ngrx/router-store": "^16.2.0",
		"@ngrx/store": "^16.2.0",
		"@ngrx/store-devtools": "^16.2.0",
		"@nguniversal/common": "^16.1.3",
		"@nguniversal/express-engine": "^16.1.3",
		"@tonaljs/tonal": "^4.6.0",
		"angular2-draggable": "^16.0.0",
		"canvas": "^2.11.2",
		"chance": "^1.1.3",
		"express": "^4.17.1",
		"fast-simplex-noise": "^4.0.0",
		"jest-environment-jsdom": "^29.6.2",
		"jsdom": "^20.0.0",
		"nanoid": "^4.0.0",
		"ngx-device-detector": "^6.0.2",
		"reflect-metadata": "^0.1.13",
		"rot-js": "^2.1.5",
		"rxjs": "~6.5.5",
		"source-map-support": "^0.5.19",
		"three": "~0.152.2",
		"tone": "^14.7.77",
		"tsconfig-paths": "^3.9.0",
		"tslib": "^2.0.0",
		"uuid": "^8.3.2",
		"zone.js": "~0.13.1"
	},
	"devDependencies": {
		"@angular-builders/jest": "^16.0.0",
		"@angular-devkit/build-angular": "^16.2.0",
		"@angular-eslint/builder": "16.1.0",
		"@angular-eslint/eslint-plugin": "16.1.0",
		"@angular-eslint/eslint-plugin-template": "16.1.0",
		"@angular-eslint/schematics": "16.1.0",
		"@angular-eslint/template-parser": "16.1.0",
		"@angular/cli": "^16.2.0",
		"@angular/compiler-cli": "^16.2.0",
		"@angular/language-service": "^16.2.0",
		"@babel/core": "^7.14.3",
		"@ngrx/schematics": "^16.2.0",
