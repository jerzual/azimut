{
  "name": "azimut",
  "version": "1.0.0",
  "description": "a turn-based survival game",
  "main": "index.js",
  "scripts": {
    "start": "ts-node fuse dev",
    "debug": "node -r ts-node/register --inspect fuse.ts dev",
    "build": "ts-node fuse build",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "test": "npm run test-client && npm run test-server",
    "test-client": "jest",
    "test-engine": "jest ./src/engine/*",
    "test-server": "mocha --reporter spec --require ./test/utils/node.ts --compilers ts:ts-node/register"
  },
  "author": "Benjamin Legrand",
  "repository": {
    "type": "git",
    "url": "https://github.com/jerzual/azimut.git"
  },
  "license": "CC-BY-NC-4.0",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/jsdom": "^11.0.4",
    "@types/mongodb": "^3.0.18",
    "@types/node": "^9.6.12",
    "@types/socket.io": "^1.4.33",
    "@types/socket.io-client": "^1.4.32",
    "@types/supertest": "^2.0.4",
    "browser-sync": "^2.24.4",
    "enzyme": "^3.3.0",
    "fuse-box": "^3.7.1",
    "inferno-devtools": "^7.1.12",
    "inferno-test-utils": "^7.1.12",
    "jest": "^24.7.1",
    "node-sass": "^4.9.0",
    "postcss-modules": "^1.4.1",
    "pug": "^2.0.3",
    "rot-js": "^0.6.5",
    "supertest": "^3.0.0",
    "terser": "^3.16.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "ts-transform-classcat": "^0.1.1",
    "ts-transform-inferno": "^4.0.2",
    "tslint": "^5.10.0",
    "typescript": "^3.4.4"
  },
  "dependencies": {
    "babylonjs": "^4.0.0-rc.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "chance": "^1.0.16",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-socket.io-session": "^1.3.3",
    "fast-simplex-noise": "^3.2.0",
    "gsap": "^1.20.2",
    "immutable": "^3.8.2",
    "inferno": "^7.1.11",
    "inferno-compat": "^7.1.12",
    "inferno-hydrate": "^7.1.11",
    "inferno-redux": "^7.1.12",
    "inferno-router": "^7.1.12",
    "inferno-server": "^7.1.11",
    "lodash": "^4.17.10",
    "method-override": "^2.3.10",
    "moment": "^2.22.1",
    "mongodb": "^3.0.7",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "prop-types": "^15.6.1",
    "rebass": "^1.0.3",
    "redux": "^4.0.1",
    "redux-devtools": "^3.5.0",
    "redux-observable": "^1.1.0",
    "redux-socket.io": "^1.4.0",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "serve-favicon": "^2.4.3",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "source-map-support": "^0.4.15",
    "sticky-session": "^1.1.2",
    "tone": "^0.12.8",
    "uuid": "^3.2.1",
    "whatwg-fetch": "^2.0.4",
    "winston": "^2.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(tsx?)$": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "testEnvironment": "node"
  }
}
