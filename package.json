{
  "name": "@jerzual/azimut",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:watch": "ng test --watch",
    "gen:config": "json2ts --input src/assets/config-schema.json --output src/assets/config.d.ts",
    "gen:sprites": "json2ts --input src/assets/sprites-schema.json --output src/assets/sprites.d.ts",
    "gen": "npm run gen:config && npm run gen:sprites",
    "serve:ssr": "ng run azimut:serve-ssr",
    "build": "ng build --configuration production && ng run azimut:server:production",
    "build:dev": "ng build && ng run azimut:server",
    "start": "node dist/server/main.js",
    "prerender": "ng run azimut:prerender"
  },
  "engines": {
    "node": ">=20.x"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.2",
    "@angular/cdk": "^18.2.2",
    "@angular/common": "^18.2.2",
    "@angular/compiler": "^18.2.2",
    "@angular/core": "^18.2.2",
    "@angular/forms": "^18.2.2",
    "@angular/platform-browser": "^18.2.2",
    "@angular/platform-browser-dynamic": "^18.2.2",
    "@angular/platform-server": "^18.2.2",
    "@angular/router": "^18.2.2",
    "@angular/ssr": "^18.2.2",
    "@ngrx/signals": "^18.0.2",
    "@tonaljs/tonal": "^4.10.0",
    "angular2-draggable": "^16.0.0",
    "canvas": "^2.11.2",
    "chance": "^1.1.11",
    "express": "^4.18.2",
    "fast-simplex-noise": "^4.0.0",
    "nanoid": "^5.0.6",
    "ngx-device-detector": "^8.0.0",
    "rot-js": "^2.2.0",
    "rxjs": "^7.8.1",
    "three": "~0.152.2",
    "tone": "^15.0.4",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.7.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.7.3",
    "@analogjs/vite-plugin-angular": "^1.7.3",
    "@analogjs/vitest-angular": "^1.7.3",
    "@angular-architects/ngrx-toolkit": "^18.0.3",
    "@angular-devkit/build-angular": "^18.2.2",
    "@angular-eslint/builder": "18.3.0",
    "@angular-eslint/eslint-plugin": "18.3.0",
    "@angular-eslint/eslint-plugin-template": "18.3.0",
    "@angular-eslint/schematics": "18.3.0",
    "@angular-eslint/template-parser": "18.3.0",
    "@angular/cli": "^18.2.2",
    "@angular/compiler-cli": "^18.2.2",
    "@angular/language-service": "^18.2.2",
    "@testing-library/angular": "^17.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.20",
    "@types/three": "~0.155.1",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitest/coverage-v8": "^2.0.5",
    "browser-sync": "^3.0.2",
    "eslint": "^8.57.0",
    "image-size": "^1.1.1",
    "jsdom": "^25.0.0",
    "json-schema-to-typescript": "^15.0.2",
    "postcss": "^8.4.44",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-node": "~10.9.2",
    "typescript": "~5.5.4",
    "vite": "^5.4.2",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  },
  "pnpm": {
    "overrides": {
      "@babel/traverse@<7.23.2": ">=7.23.2",
      "follow-redirects@<1.15.4": ">=1.15.4",
      "axios@>=1.0.0 <1.6.0": ">=1.6.0",
      "ip@=2.0.0": ">=2.0.1"
    }
  }
}
