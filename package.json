{
	"name": "@jerzual/azimut",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"serve": "ng serve",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"test:watch": "ng test --watch",
		"gen:config": "json2ts --input src/assets/config-schema.json --output src/assets/config.d.ts",
		"gen:sprites": "json2ts --input src/assets/sprites-schema.json --output src/assets/sprites.d.ts",
		"gen": "npm run gen:config && npm run gen:sprites",
		"serve:ssr": "ng run azimut:serve-ssr",
		"build": "ng build --configuration production && ng run azimut:server:production",
		"build:dev": "ng build && ng run azimut:server",
		"start": "node dist/server/main.js",
		"prerender": "ng run azimut:prerender"
	},
	"engines": {
		"node": ">=20.x"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^17.2.1",
		"@angular/cdk": "^17.2.0",
		"@angular/common": "^17.2.1",
		"@angular/compiler": "^17.2.1",
		"@angular/core": "^17.2.1",
		"@angular/forms": "^17.2.1",
		"@angular/platform-browser": "^17.2.1",
		"@angular/platform-browser-dynamic": "^17.2.1",
		"@angular/platform-server": "^17.2.1",
		"@angular/router": "^17.2.1",
		"@angular/ssr": "^17.2.0",
		"@ngneat/effects": "^2.1.2",
		"@ngneat/elf": "^2.3.2",
		"@ngneat/elf-devtools": "^1.3.0",
		"@ngneat/elf-entities": "^4.5.0",
		"@ngneat/elf-requests": "^1.8.0",
		"@tonaljs/tonal": "^4.6.0",
		"angular2-draggable": "^16.0.0",
		"canvas": "^2.11.2",
		"chance": "^1.1.3",
		"express": "^4.17.1",
		"fast-simplex-noise": "^4.0.0",
		"nanoid": "^4.0.0",
		"ngx-device-detector": "^7.0.0",
		"reflect-metadata": "^0.1.13",
		"rot-js": "^2.1.5",
		"rxjs": "^7.8.1",
