		"eslint": "^8.39.0",
		"image-size": "^1.0.0",
		"jest": "^29.6.2",
		"jest-marbles": "^2.5.1",
		"json-schema-to-typescript": "^10.1.4",
		"postcss": "^8.3.0",
		"supertest": "^6.1.3",
		"ts-jest": "^29.1.1",
		"ts-node": "~10.0.0",
		"typescript": "~4.9.5"
	}
}
