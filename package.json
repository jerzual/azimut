{
  "name": "azimut-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:watch": "jest --watch",
    "gen:config": "json2ts --input src/assets/config-schema.json --output src/assets/config.d.ts",
    "gen:sprites": "json2ts --input src/assets/sprites-schema.json --output src/assets/sprites.d.ts",
    "gen": "npm run gen:config && npm run gen:sprites",
    "serve:ssr": "ng run azimut:serve-ssr",
    "build": "ng build --configuration production && ng run azimut:server:production",
    "build:dev": "ng build && ng run azimut:server",
    "start": "node dist/server/main.js",
    "prerender": "ng run azimut:prerender"
  },
  "engines": {
    "node": "18.x"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.8",
    "@angular/cdk": "^15.2.8",
    "@angular/common": "^15.2.8",
    "@angular/compiler": "^15.2.8",
    "@angular/core": "^15.2.8",
    "@angular/forms": "^15.2.8",
    "@angular/platform-browser": "^15.2.8",
    "@angular/platform-browser-dynamic": "^15.2.8",
    "@angular/platform-server": "^15.2.8",
    "@angular/router": "^15.2.8",
    "@ngrx/component-store": "^15.4.0",
    "@ngrx/effects": "^15.4.0",
    "@ngrx/entity": "^15.4.0",
    "@ngrx/router-store": "^15.4.0",
    "@ngrx/store": "^15.4.0",
    "@ngrx/store-devtools": "^15.4.0",
    "@nguniversal/common": "^15.2.1",
    "@nguniversal/express-engine": "^15.2.1",
    "@tonaljs/tonal": "^4.6.0",
    "angular-three": "^1.9.16",
    "angular2-draggable": "^2.3.2",
    "canvas": "^2.11.2",
    "chance": "^1.1.3",
    "express": "^4.17.1",
    "fast-simplex-noise": "^4.0.0",
    "jest-environment-jsdom": "^29.0.2",
    "jsdom": "^20.0.0",
    "nanoid": "^4.0.0",
    "ngx-device-detector": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "rot-js": "^2.1.5",
    "rxjs": "~6.5.5",
    "source-map-support": "^0.5.19",
    "three": "0.141.0",
    "tone": "^14.7.77",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^15.0.0",
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.6",
    "@angular/compiler-cli": "^15.2.8",
    "@angular/language-service": "^15.2.8",
    "@babel/core": "^7.14.3",
    "@ngrx/schematics": "^15.4.0",
    "@nguniversal/builders": "^15.2.1",
    "@rx-angular/eslint-plugin": "^1.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/node": "^14.17.0",
    "@types/rot-js": "^2.0.1",
    "@types/three": "0.129.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "image-size": "^1.0.0",
    "jest": "^28.1.3",
    "jest-marbles": "^2.5.1",
    "json-schema-to-typescript": "^10.1.4",
    "postcss": "^8.3.0",
    "supertest": "^6.1.3",
    "ts-jest": "^28.0.8",
    "ts-node": "~10.0.0",
    "typescript": "~4.9.5"
  }
}
