		"@angular-eslint/template-parser": "20.1.1",
		"@angular/cli": "^19.2.15",
		"@angular/compiler-cli": "^19.2.14",
		"@eslint/js": "^9.30.1",
		"@playwright/test": "^1.53.2",
		"@testing-library/angular": "^17.4.0",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^24.0.10",
