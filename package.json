{
  "name": "azimut-angular",
  "version": "0.0.0",
  "author": "Benjamin Legrand",
  "repository": {
    "type": "git",
    "url": "https://github.com/jerzual/azimut.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:watch": "jest --watch",
    "gen:config": "json2ts --input src/assets/config-schema.json --output src/assets/config.d.ts",
    "gen:sprites": "json2ts --input src/assets/sprites-schema.json --output src/assets/sprites.d.ts",
    "gen": "npm run gen:config && npm run gen:sprites",
    "serve:ssr": "ng run azimut:serve-ssr",
    "build": "ng build --configuration production && ng run azimut:server:production",
    "build:dev": "ng build && ng run azimut:server",
    "start": "node dist/server/main.js",
    "prerender": "ng run azimut:prerender"
  },
  "engines": {
    "node": "14.x"
  },
  "private": true,
  "dependencies": {
    "@angular-three/core": "^6.0.1",
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/platform-server": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@ngrx/component-store": "^14.3.0",
    "@ngrx/effects": "^14.3.0",
    "@ngrx/entity": "^14.3.0",
    "@ngrx/router-store": "^14.3.0",
    "@ngrx/store": "^14.3.0",
    "@ngrx/store-devtools": "^14.3.0",
    "@nguniversal/common": "^14.1.0",
    "@nguniversal/express-engine": "^14.1.0",
    "@tonaljs/tonal": "^4.6.0",
    "angular2-draggable": "^2.3.2",
    "canvas": "^2.6.1",
    "chance": "^1.1.3",
    "express": "^4.17.1",
    "fast-simplex-noise": "^4.0.0",
    "jsdom": "^16.6.0",
    "ngx-device-detector": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rot-js": "^2.1.5",
    "rxjs": "~6.5.5",
    "source-map-support": "^0.5.19",
    "three": "0.141.0",
    "tone": "^14.7.77",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^12.0.0",
    "@angular-devkit/build-angular": "^14.2.1",
    "@angular-eslint/builder": "14.0.3",
    "@angular-eslint/eslint-plugin": "14.0.3",
    "@angular-eslint/eslint-plugin-template": "14.0.3",
    "@angular-eslint/schematics": "14.0.3",
    "@angular-eslint/template-parser": "14.0.3",
    "@angular/cli": "^14.2.1",
    "@angular/compiler-cli": "^14.2.0",
    "@angular/language-service": "^14.2.0",
    "@babel/core": "^7.14.3",
    "@ngrx/schematics": "^14.3.0",
    "@nguniversal/builders": "^14.1.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^26.0.23",
