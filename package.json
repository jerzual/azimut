{
	"name": "@jerzual/azimut",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"build": "ng build",
		"start": "ng serve",
		"test": "ng test",
		"lint": "ng lint",
		"lint:fix": "eslint --fix \"src/**/*.{ts,html}\"",
		"lint:gha": "eslint \"src/**/*.{ts,html}\" --format gha; exit 0",
		"format": "prettier --write \"src/**/*\"",
		"format:check": "prettier --check \"src/**/*\"",
		"e2e": "ng e2e",
		"test:watch": "ng test --watch",
		"gen:config": "json2ts --input src/assets/config-schema.json --output src/assets/config.schema.ts",
		"gen:sprites": "json2ts --input src/assets/sprites-schema.json --output src/assets/sprites.schema.ts",
		"gen": "npm run gen:config && npm run gen:sprites",
		"serve": "node dist/server/server.mjs"
	},
	"engines": {
		"node": ">=22.x"
	},
	"private": true,
	"dependencies": {
		"@angular/cdk": "^19.2.19",
		"@angular/common": "^19.2.14",
		"@angular/compiler": "^19.2.14",
		"@angular/core": "^19.2.14",
		"@angular/forms": "^19.2.14",
		"@angular/platform-browser": "^19.2.14",
		"@angular/platform-browser-dynamic": "^19.2.14",
		"@angular/platform-server": "^19.2.14",
		"@angular/router": "^19.2.14",
		"@angular/ssr": "^19.2.15",
		"@fastify/http-proxy": "^11.3.0",
		"@fastify/static": "^8.2.0",
		"@ngrx/signals": "^19.2.1",
		"@tonaljs/tonal": "^4.10.0",
		"angular-three": "^3.7.2",
		"angular2-draggable": "^16.0.0",
		"chance": "^1.1.13",
		"fast-simplex-noise": "^4.0.0",
		"fastify": "^5.4.0",
		"nanoid": "^5.1.5",
		"ngx-device-detector": "^9.0.0",
		"ngxtension": "4.0.0",
		"rot-js": "^2.2.1",
		"rxjs": "^7.8.2",
		"three": "~0.172.0",
		"three-stdlib": "^2.36.0",
		"tone": "^15.1.22",
		"tslib": "^2.3.0",
		"zone.js": "~0.15.1"
	},
	"devDependencies": {
		"@analogjs/platform": "^1.19.0",
