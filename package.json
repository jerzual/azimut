    "json-schema-to-typescript": "^10.1.4",
    "postcss": "^8.3.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-node": "~10.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4"
  }
}
