{
  "name": "plague",
  "version": "1.0.0",
  "description": "a turn-based survival game",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.bundle.js",
    "serve": " npm run build-server &&  npm run start ",
    "postinstall": "npm run build-client && npm run build-server",
    "build-client": "webpack -p --define process.env.NODE_ENV=\"production\" --progress --colors",
    "build-server": "webpack --config webpack.server.config.js --progress --colors",
    "build-test": "tsc test/index.ts --outDir dist/test --module commonjs",
    "watch": "webpack --watch --progress",
    "test": "npm run test-client && npm run test-server",
    "test-client": "karma start",
    "test-server": "mocha --reporter spec --compilers ts:ts-node/register ./**/*.spec.ts"
  },
